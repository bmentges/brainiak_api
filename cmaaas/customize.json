{
    "mappings": [
        {
            "match": "/eureka/ProcessoSeletivo",
            "pages": {
                "list": "eureka_ProcessoSeletivo_list"
            }
        },
        {
            "match": "/eureka/AssuntoEureka",
            "pages": {
                "list": "eureka_AssuntoEureka_list"
            }
        },
        {
            "match": "/eureka/AssuntoEureka/.*$",
            "pages": {
                "edit": "eureka_AssuntoEureka_instance",
                "add": "eureka_AssuntoEureka_instance"
            }
        },
        {
            "match": "/eureka/Disciplina",
            "pages": {
                "list": "eureka_Disciplina_list"
            }
        },
        {
            "match": "/eureka/Disciplina/.*$",
            "pages": {
                "edit": "eureka_Disciplina_instance",
                "add": "eureka_Disciplina_instance"
            }
        },
        {
            "match": "/eureka/EditoriaEureka",
            "pages": {
                "list": "eureka_EditoriaEureka_list"
            }
        },
        {
            "match": "/eureka/EditoriaEureka/.*$",
            "pages": {
                "edit": "eureka_EditoriaEureka_instance",
                "add": "eureka_EditoriaEureka_instance"
            }
        },
        {
            "match": "/eureka/Edicao",
            "pages": {
                "list": "eureka_Edicao_list"
            }
        },
        {
            "match": "/eureka/Edicao/.*$",
            "pages": {
                "edit": "eureka_Edicao_instance",
                "add": "eureka_Edicao_instance"
            }
        },
        {
            "match": "/eureka/Prova",
            "pages": {
                "list": "eureka_Prova_list"
            }
        },
        {
            "match": "/eureka/Prova/.*$",
            "pages": {
                "edit": "eureka_Prova_instance",
                "add": "eureka_Prova_instance"
            }
        }
    ],
    "customizations": {
        "eureka_ProcessoSeletivo_list": {
            "columns": [
                {
                    "key": "title",
                    "title": "Título do Processo Seletivo"
                }
            ]
        },
        "eureka_AssuntoEureka_list": {
            "columns": [
                {
                    "key": "title",
                    "title": "Título do Assunto"
                }
            ]
        },
        "eureka_AssuntoEureka_instance": {
            "form": [
                "rdfs:label",
                "base:descricao",
                "base:slug_topico",
                "eureka:Keywords",
                {
                    "key": "eureka:PertenceAEditoriaOuDisciplina",
                    "type": "suggest-brainiak",
                    "options": {
                        "relItem": "relatedInstance",
                        "relItems": "suggest"
                    }
                }
            ]
        },
        "eureka_Disciplina_list": {
            "columns": [
                {
                    "key": "title",
                    "title": "Título da Disciplina"
                }
            ]
        },
        "eureka_Disciplina_instance": {
            "form": [
                "rdfs:label",
                "eureka:Folder_ID",
                "eureka:URL"
            ]
        },
        "eureka_EditoriaEureka_list": {
            "columns": [
                {
                    "key": "title",
                    "title": "Título da Editoria"
                }
            ]
        },
        "eureka_EditoriaEureka_instance": {
            "form": [
                "rdfs:label",
                "eureka:Folder_ID",
                "eureka:URL"
            ]
        },
         "eureka_Edicao_list": {
            "columns": [
                {
                    "key": "title",
                    "title": "Título da Edição"
                }
            ]
        },
       "eureka_Edicao_instance": {
            "form": [
                "rdfs:label",
                "eureka:Ano",
                {
                    "key": "eureka:PertenceAoProcessoSeletivo",
                    "type": "suggest-brainiak",
                    "options": {
                        "relItem": "relatedInstance",
                        "relItems": "suggest"
                    }
                }

            ]
        },
        "eureka_Prova_list": {
            "columns": [
                {
                    "key": "title",
                    "title": "Título da Prova"
                }
            ]
        },
        "eureka_Prova_instance": {
            "form": [
                "rdfs:label",
                "base:descricao",
                "base:slug_topico",
                "eureka:Keywords",
                "eureka:Disclaimer",
                {
                    "key": "eureka:PertenceAEdicao",
                    "type": "suggest-brainiak",
                    "options": {
                        "relItem": "relatedInstance",
                        "relItems": "suggest"
                    }
                },
                {
                    "key": "eureka:PertenceADisciplina",
                    "type": "suggest-brainiak",
                    "options": {
                        "relItem": "relatedInstance",
                        "relItems": "suggest"
                    }
                }
            ]
        }
    }
}
